# ArtFight Webhook Service Environment Variables
# Copy this file to .env and customize it

# Server settings
HOST=0.0.0.0
PORT=8000
DEBUG=false

# Request intervals (in seconds)
REQUEST_INTERVAL=300
TEAM_CHECK_INTERVAL=3600
TEAM_SWITCH_THRESHOLD=24

# ArtFight settings
ARTFIGHT_BASE_URL=https://artfight.net

# Cache settings
CACHE_DB_PATH=cache/artfight_cache.db

# Authentication settings
# Get these cookies from your browser after logging into ArtFight
LARAVEL_SESSION=your_laravel_session_cookie_here
CF_CLEARANCE=your_cloudflare_clearance_cookie_here

# Discord webhook URLs (you can also set these in config.toml)
# TEAM_WEBHOOK_URL=https://discord.com/api/webhooks/YOUR_TEAM_WEBHOOK_URL

# User webhooks (you can also set these in config.toml)
# USER_WEBHOOK_1=https://discord.com/api/webhooks/USER1_WEBHOOK_URL
# USER_WEBHOOK_2=https://discord.com/api/webhooks/USER2_WEBHOOK_URL 